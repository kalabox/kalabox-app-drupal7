{
  "appName": "my-drupal-7-app",
  "appPlugins": [
    "kalabox-plugin-dbenv",
    "kalabox-plugin-git",
    "kalabox-plugin-drush",
    "kalabox-plugin-rsync"
  ],
  "shareIgnores": [
    "sites/default/files/*",
    ".git"
  ],
  "pluginConf": {
    "kalabox-plugin-drush": {
      "drush-version": "drush6"
    }
  },
  "appComponents": {
    "data": {
      "image": {
        "name": "kalabox/data:stable"
      }
    },
    "db": {
      "image": {
        "name": "kalabox/mariadb:stable"
      }
    },
    "appserver": {
      "image": {
        "name": "kalabox/php-appserver:stable"
      },
      "proxy": [
        {
          "port": "80/tcp",
          "default": true
        }
      ]
    }
  }
}
